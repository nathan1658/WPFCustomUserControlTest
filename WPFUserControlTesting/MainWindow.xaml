<Window x:Class="WPFUserControlTesting.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WPFUserControlTesting"
        mc:Ignorable="d"
        
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <local:TestUserControl LabelString="{Binding ColumnText}" Value="{Binding Value}" DataGridItems="{Binding ChartBarRecordList}">
            
        </local:TestUserControl>
    </Grid>
</Window>
